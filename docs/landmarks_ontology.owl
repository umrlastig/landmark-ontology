    <!-- http://purl.org/choucas.ign.fr/oor#aCommeCompositionEnsemble -->


    <owl:ObjectProperty rdf:about="http://purl.org/choucas.ign.fr/oor#aCommeCompositionEnsemble">
        <rdfs:domain rdf:resource="http://purl.org/choucas.ign.fr/oor#forêt"/>
        <rdfs:range rdf:resource="http://purl.org/choucas.ign.fr/oor#CompositionEnsemble"/>
        <rdfs:comment xml:lang="en">To characterize the dominant stand composition of vegetation types (e.g., deciduous, coniferous) within a forest area.</rdfs:comment>
        <rdfs:comment>Pour modéliser la composition dominante des types de formation végétale (ex. feuillus, conifères).</rdfs:comment>
        <rdfs:label xml:lang="fr">a comme composition dominante</rdfs:label>
        <rdfs:label xml:lang="en">type of stand composition</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/choucas.ign.fr/oor#aPourCouleur -->


    <owl:ObjectProperty rdf:about="http://purl.org/choucas.ign.fr/oor#aPourCouleur">
